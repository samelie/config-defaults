{
  "name": "@adddog/config-defaults",
  "version": "0.0.8",
  "type": "module",
  "private": false,
  "homepage": "https://github.com/samelie/config-defaults#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/samelie/config-defaults.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/config-defaults/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "types": "tsc -p tsconfig.typecheck.json",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "prepublishOnly": "pnpm run lint && pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "knip": "knip",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "exports": {
    "./knip.config": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./knip.config.js": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./vite.config": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    },
    "./vite.config.js": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    },
    "./vitest.config": {
      "import": {
        "types": "./dist/vitest.config.d.ts",
        "default": "./dist/vitest.config.mjs"
      }
    },
    "./vitest.config.js": {
      "import": {
        "types": "./dist/vitest.config.d.ts",
        "default": "./dist/vitest.config.mjs"
      }
    }
  },
  "devDependencies": {
    "@adddog/build-configs": "workspace:*",
    "@adddog/eslint": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vitest/browser-playwright": "4.0.0-beta.17",
    "@vitest/ui": "^4.0.9",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "knip": "^5.69.1",
    "package-json-type": "^1.0.3",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@playwright/test": "^1.56.1",
    "lodash-es": "^4.17.21"
  }
}
