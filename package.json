{
  "name": "@adddog/config-defaults",
  "version": "0.0.5",
  "type": "module",
  "private": false,
  "homepage": "https://github.com/samelie/config-defaults#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/samelie/config-defaults.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/config-defaults/issues"
  },
  "scripts": {
    "build": "rad-build",
    "test": "vitest run",
    "types": "tsc -p tsconfig.typecheck.json",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "exports": {
    "./knip.config": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./knip.config.js": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./vite.config": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    },
    "./vite.config.js": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    }
  },
  "devDependencies": {
    "@adddog/build-configs": "workspace:*",
    "@adddog/eslint": "workspace:*",
    "@playwright/test": "^1.56.0",
    "@rad/config": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.8.0",
    "@vitest/browser": "4.0.0-beta.17",
    "@vitest/browser-playwright": "4.0.0-beta.17",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "happy-dom": "^19.0.2",
    "knip": "^5.65.0",
    "package-json-type": "^1.0.3",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.1.10",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^4.0.0-beta.18"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
