{
  "name": "@adddog/config-defaults",
  "version": "0.0.8",
  "type": "module",
  "private": false,
  "homepage": "https://github.com/samelie/config-defaults#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/samelie/config-defaults.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/config-defaults/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "echo '⚠️⚠️⚠️no test vitest --watch=false'",
    "types": "tsc -p tsconfig.typecheck.json",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "prepublishOnly": "pnpm run lint && pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "knip": "knip"
  },
  "exports": {
    "./knip.config": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./knip.config.js": {
      "import": {
        "types": "./dist/knip.config.d.ts",
        "default": "./dist/knip.config.mjs"
      }
    },
    "./vite.config": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    },
    "./vite.config.js": {
      "import": {
        "types": "./dist/vite.config.d.ts",
        "default": "./dist/vite.config.mjs"
      }
    },
    "./vitest.config": {
      "import": {
        "types": "./dist/vitest.config.d.ts",
        "default": "./dist/vitest.config.mjs"
      }
    },
    "./vitest.config.js": {
      "import": {
        "types": "./dist/vitest.config.d.ts",
        "default": "./dist/vitest.config.mjs"
      }
    }
  },
  "devDependencies": {
    "@adddog/build-configs": "^0.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.10",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "knip": "^5.82.1",
    "package-json-type": "^1.1.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@playwright/test": "^1.58.0",
    "lodash-es": "^4.17.23"
  }
}
